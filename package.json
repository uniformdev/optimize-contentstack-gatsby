{
  "name": "optimize-contentstack-gatsby",
  "description": "Uniform Optimize Gatsby starter site for Contentstack",
  "version": "1.0.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "run-s generate:intents clean build:gatsby",    
    "develop": "run-s generate:intents develop:gatsby",    
    "build:gatsby": "gatsby build",
    "develop:gatsby": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "generate:intents": "uniform optimize manifest download --output ./src/lib/intentManifest.json --apiKey $UNIFORM_API_KEY",
    "ci:build": "run-s generate:intents clean build:gatsby"
  },
  "dependencies": {
    "@analytics/google-analytics": "^0.5.2",
    "@analytics/segment": "^0.6.0",  
    "@n8tb1t/use-scroll-position": "^2.0.3",    
    "@uniformdev/optimize-tracker-analytics": "^3.0.0",
    "@uniformdev/optimize-tracker-browser": "^3.0.0",
    "@uniformdev/optimize-tracker-contentstack": "^3.0.0",
    "@uniformdev/optimize-tracker-react": "^3.0.0",
    "analytics": "^0.7.2",
    "dotenv": "^8.2.0",
    "gatsby": "^2.32.3",
    "gatsby-plugin-postcss": "^3.6.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-source-contentstack": "^2.3.1",
    "js-cookie": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "@types/react": "^16.9.55",
    "@uniformdev/cli": "^3.0.0",
    "autoprefixer": "^10.1.0",
    "babel-runtime": "^6.26.0",
    "gatsby-cli": "^2.19.1",    
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.5"    
  }
}
